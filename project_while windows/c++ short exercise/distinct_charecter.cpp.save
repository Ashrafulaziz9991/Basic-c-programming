#include<bits/stdc++.h>
using namespace std;

int main()
{
    string s;
    getline(cin, s);

    int d_c = 0;
    sort(s.begin(), s.end());

    for(int i=0; i<s.size() ;i++)
    {
        if(s[i]!=s[i+1]){
            d_c++;
        }
    }
    if(d_c%2==0)
        cout<<"CHAT WITH HER!\n";
    else
        cout<<"IGNORE HIM!\n";

    return 0;
}


